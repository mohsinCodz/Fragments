<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/home_Fragment">

    <fragment
        android:id="@+id/home_Fragment"
        android:name="mohsin.code.fragments.home_Fragment"
        android:label="Home Fragment"
        tools:layout="@layout/fragment_home_" >
        <action
            android:id="@+id/action_home_Fragment_to_song_list_fragment"
            app:destination="@id/song_list_fragment" />
    </fragment>
    <fragment
        android:id="@+id/song_list_fragment"
        android:name="mohsin.code.fragments.SongListFragment"
        android:label="Songs List"
        tools:layout="@layout/songslist" >
        <action
            android:id="@+id/action_song_list_fragment_to_song_details"
            app:destination="@id/song_details" />
    </fragment>
    <fragment
        android:id="@+id/song_details"
        android:name="mohsin.code.fragments.song_details"
        android:label="Song Details"
        tools:layout="@layout/fragment_song_details" >
        <action
            android:id="@+id/action_song_details_to_home_Fragment"
            app:destination="@id/home_Fragment" />
    </fragment>
</navigation>